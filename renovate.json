{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "local>brave/renovate-config"
  ],
  "schedule": [
    "every 7 day"
  ],
  "baseBranches": [
    "master"
  ],
  "pre-commit": {
    "enabled": true
  },
  "pip_requirements": {
    "fileMatch": [
      "requirements.*"
    ]
  },
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^regex$"
      ],
      "enabled": false
    }
  ]
}
